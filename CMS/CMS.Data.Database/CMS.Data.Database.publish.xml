<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>CMSDB</TargetDatabaseName>
    <DeployScriptFileName>CMS.Data.Database.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=.;Database=CMSDB;Integrated Security=true</TargetConnectionString>
    <BlockOnPossibleDataLoss>False</BlockOnPossibleDataLoss>
    <ScriptNewConstraintValidation>False</ScriptNewConstraintValidation>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <ExcludeUsers>True</ExcludeUsers>
    <IgnoreRoleMembership>True</IgnoreRoleMembership>
    <ExcludeCertificates>True</ExcludeCertificates>
  </PropertyGroup>
</Project>